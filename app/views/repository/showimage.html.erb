<% @page_title = 'Browsing repository' %>

<div id="content">
<%= render :partial => 'breadcrumbs', :locals => { :trail => true, :last_clickable => false } %>
<%= render :partial => 'meta', :locals => {:node => @file} %>

<p />
<%= "<img src=\"#{url_for( :action => 'send_data_to_browser', :path => params[:path] )}\" alt=\"#{@file.name}\" />" %>
</div>
